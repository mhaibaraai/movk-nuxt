{
  "name": "@movk/nuxt",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.18.0",
  "description": "Movk Nuxt module",
  "license": "MIT",
  "repository": "movk/nuxt",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nr build",
    "build": "nuxt-module-build build",
    "dev": "nr dev:prepare && nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "release": "release-it --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxt typecheck playground",
    "up": "taze major -wIr",
    "clean": "tsx scripts/rm.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@iconify-json/lucide": "catalog:",
    "@iconify-json/simple-icons": "catalog:",
    "@movk/core": "catalog:",
    "@nuxt/image": "catalog:",
    "@nuxt/kit": "catalog:",
    "@nuxt/ui": "catalog:",
    "@nuxtjs/i18n": "catalog:",
    "@vueuse/core": "catalog:",
    "defu": "catalog:",
    "lru-cache": "catalog:",
    "nuxt-auth-utils": "catalog:",
    "ufo": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@antfu/ni": "catalog:",
    "@nuxt/devtools": "catalog:",
    "@nuxt/eslint": "catalog:",
    "@nuxt/module-builder": "catalog:",
    "@nuxt/schema": "catalog:",
    "@nuxt/test-utils": "catalog:",
    "@types/node": "catalog:",
    "@types/sortablejs": "catalog:",
    "@vueuse/nuxt": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "fast-glob": "catalog:",
    "nuxt": "catalog:",
    "sortablejs": "catalog:",
    "taze": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  }
}
