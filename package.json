{
  "name": "@movk/nuxt",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.28.0",
  "description": "Modular engineering suite for Nuxt 4 with schema-driven forms, API integration, UI components and composables",
  "author": "YiXuan <mhaibaraai@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-nuxt.git"
  },
  "homepage": "https://nuxt.mhaibaraai.cn",
  "engines": {
    "node": "^20.x || ^22.x || ^24.x"
  },
  "bugs": {
    "url": "https://github.com/mhaibaraai/movk-nuxt/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt4",
    "zod",
    "form",
    "auto-form",
    "schema-driven",
    "validation",
    "api",
    "fetch",
    "auth",
    "upload",
    "download",
    "composables",
    "ui-components"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@nuxt/ui": ">=4.3.0",
    "zod": ">=4.3.5"
  },
  "scripts": {
    "prepack": "pnpm build",
    "build": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare docs",
    "dev:build": "nuxt build playground",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "release": "release-it && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc -b --noEmit && nuxt typecheck playground && nuxt typecheck docs",
    "clean": "movk-clean",
    "test": "vitest"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.84",
    "@internationalized/date": "^3.10.1",
    "@movk/core": "^1.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.3.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "defu": "^6.1.4",
    "nuxt-auth-utils": "^0.5.26",
    "tailwindcss": "^4.1.18",
    "scule": "^1.3.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/kit": "^4.2.2",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.23.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.4",
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.4.0",
    "eslint": "9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.2.2"
  }
}
