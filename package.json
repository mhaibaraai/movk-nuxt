{
  "name": "@movk/nuxt",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@10.26.1",
  "description": "Schema-driven form and table generation module for Nuxt 4",
  "author": "YiXuan <mhaibaraai@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-nuxt.git"
  },
  "homepage": "https://nuxt.mhaibaraai.cn",
  "engines": {
    "node": "^20.x || ^22.x"
  },
  "bugs": {
    "url": "https://github.com/mhaibaraai/movk-nuxt/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt4",
    "zod",
    "form",
    "table",
    "auto-form",
    "schema-driven",
    "validation"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@nuxt/ui": ">=4.2.1",
    "zod": ">=4.1.13"
  },
  "scripts": {
    "prepack": "nr build",
    "build": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare docs",
    "dev:build": "nuxt build playground",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "release": "release-it && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxt typecheck playground",
    "clean": "tsx scripts/rm.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.82",
    "@internationalized/date": "^3.10.1",
    "@movk/core": "^1.0.2",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.3.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "defu": "^6.1.4",
    "nuxt-auth-utils": "^0.5.26",
    "scule": "^1.3.0",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@antfu/ni": "^28.0.0",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/kit": "^4.2.2",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.4",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "9.39.1",
    "release-it": "^19.1.0",
    "fast-glob": "^3.3.3",
    "nuxt": "^4.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  }
}
