{
  "name": "@movk/nuxt",
  "type": "module",
  "version": "1.1.2",
  "packageManager": "pnpm@10.29.2",
  "description": "Nuxt 4 modular engineering suite â€” schema-driven auto forms (Zod v4), API integration with auth & progress tracking, standalone UI components, and utility composables",
  "author": "YiXuan <mhaibaraai@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-nuxt.git"
  },
  "homepage": "https://nuxt.mhaibaraai.cn",
  "engines": {
    "node": "^22.x || ^24.x"
  },
  "bugs": {
    "url": "https://github.com/mhaibaraai/movk-nuxt/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt4",
    "zod",
    "form",
    "auto-form",
    "schema-driven",
    "validation",
    "api",
    "fetch",
    "auth",
    "upload",
    "download",
    "composables",
    "ui-components"
  ],
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "style": "./dist/runtime/style.css",
      "import": "./dist/module.mjs"
    },
    "./runtime/*": "./dist/runtime/*",
    "./components/*": "./dist/runtime/components/*",
    "./composables": {
      "types": "./dist/runtime/composables/index.d.ts",
      "import": "./dist/runtime/composables/index.js"
    },
    "./composables/*": {
      "types": "./dist/runtime/composables/*.d.ts",
      "import": "./dist/runtime/composables/*.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ],
      "runtime/*": [
        "./dist/runtime/*"
      ],
      "components/*": [
        "./dist/runtime/components/*"
      ],
      "composables": [
        "./dist/runtime/composables/index.d.ts"
      ],
      "composables/*": [
        "./dist/runtime/composables/*.d.ts"
      ]
    }
  },
  "style": "./dist/runtime/index.css",
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@nuxt/ui": ">=4.3.0",
    "zod": ">=4.3.5"
  },
  "scripts": {
    "prepack": "pnpm build",
    "build": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare docs",
    "dev:build": "nuxt build playground",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "release": "release-it && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxt typecheck playground && nuxt typecheck docs",
    "clean": "movk-clean",
    "test": "vitest"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.89",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/tabler": "^1.2.26",
    "@internationalized/date": "^3.11.0",
    "@movk/core": "^1.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.4.0",
    "@vueuse/core": "^14.2.1",
    "@vueuse/nuxt": "^14.2.0",
    "defu": "^6.1.4",
    "motion-v": "^1.10.3",
    "nuxt-auth-utils": "^0.5.28",
    "nuxt-og-image": "^5.1.13",
    "reka-ui": "2.8.0",
    "scule": "^1.3.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.15.1",
    "@nuxt/kit": "^4.3.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.1",
    "@nuxt/test-utils": "^4.0.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.5",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "jsdom": "^28.0.0",
    "nuxt": "^4.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
